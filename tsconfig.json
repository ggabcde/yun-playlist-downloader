{
  "include": ["./ts-src/**/*.ts"],
  "compilerOptions": {
    "typeRoots":[
      "./node_modules/@types",
      "./ts-src/ts-patch/"
    ],
    "allowJs": true,
    "module": "commonjs",
    "outDir": "js-lib",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target":"ES2017"
  }
}
